<body>
    <script>
        let arr = [
        [1, 3, 4],
        [3, 4, 5],
        [5, 5, 6]
    ];
    let DC = arr[0][arr.length-1]
     for ( let i = 0; i < arr.length ; i++ ) {
        for ( j = 0 ; j < arr[i].length-1 ; j++){
            i = 3-1-j;
            DC = DC + arr[i][j];
        } console.log( " đường chéo phụ là :" + DC)
     }
    </script>
</body>
</html>
