<body>
    <script>
         let arr = [
        [1, 3, 4],
        [3, 4, 5],
        [5, 5, 6]
    ];
   for ( let j = 0 ; j < arr[0].length; j++){
    let max = arr[0][j];
    for( i = 0; i< arr.length; i++){
        if( max > arr[i][j]){
            max = arr[i][j]
        }
    } console.log( " giá trị lớn nhất tại cột " + j + "là : " + max)
   }
    </script>
</body>
</html>
